		<!-- For form validation -->
		<script>
			var isFormAcceptable = 1;
			
			function checkRegistrationStatus(status)
			{
				if ( status )
					alert("You have been registered successfully!");
			
			}
			
			function validateName()
			{
				var name = document.getElementById("name");
				var non_whitespace_name = name.value.replace(/\s/g, '');
				var return_value = false;
				
				if ( non_whitespace_name.trim().length >= 5 )
				{
					name.className = "uk-input uk-form-success";
					name.title = "It's acceptable!";
					return_value = 1;
				}
				else
				{
					name.className = "uk-input uk-form-danger";
					name.title = "Your name needs to have atleast 5 non-whitespace charachters";
					return_value = 0;
				}

				isFormAcceptable = isFormAcceptable & return_value;
				
				return return_value;
			}
		
			function checkPasswordValidity()
			{
				var password = document.getElementById("password");
				let regex = /.*([a-z]+[A-Z]+[0-9]+)|([A-Z]+[0-9]+[a-z]+)|([0-9]+[a-z]+[A-Z]+)|([a-z]+[0-9]+[A-Z]+)|([0-9]+[A-Z]+[a-z]+)|([A-Z]+[a-z]+[0-9]+).*/;
			
				if ( password.value.match(regex) != null && password.value.length >= 5 )
				{
					password.className = "uk-input uk-form-success";
					password.title = "Your password looks good!";
					return_value = 1;
				}
				else
				{
					password.className = "uk-input uk-form-danger";
					password.title = "Ensure that your password contains atleast 5 charachters containing atleast 1 capital letter, 1 small letter and 1 digit.";
					return_value = 0;
				}

				isFormAcceptable = isFormAcceptable & return_value;
				
				return return_value;		
			}
		
			function checkConfirmPassword()
			{
				var password = document.getElementById("password");
				var confirm_password = document.getElementById("confirm_password");
			
				if ( password.value == confirm_password.value )
				{
					confirm_password.className = "uk-input uk-form-success";
					confirm_password.title = "Sweet!";
					return_value = 1;
				}
				else
				{
					confirm_password.className = "uk-input uk-form-danger";
					confirm_password.title = "Your passwords don't match. Please retry.";
					return_value = 0;
				}
				
				isFormAcceptable = isFormAcceptable & return_value;
				
				return return_value;
			}
			
			function checkFileType()
			{
				let regex = /(\.pdf)$/;
				var file = document.getElementById("id_proof");
				
				if ( file.value.match(regex) != null )
				{
					file.className = "uk-input uk-form-success";
					file.title = "This file is acceptable!";
					return_value = 1;
				}
				else
				{
					file.className = "uk-input uk-form-danger";
					file.title = "Only PDF files are acceptable!";
					return_value = 0;
				}
				
				isFormAcceptable = isFormAcceptable & return_value;
				
				return return_value; 
			}
		
			function checkFormValidity()
			{
				if (isForAcceptable)
					return true;
				else
					return false;
			}
		
		</script>

